{
  "name": "affine-reader",
  "version": "2.5.1",
  "description": "A simple reader for the blocksuite format",
  "scripts": {
    "test": "vitest",
    "build": "pnpm bundle && pnpm dts",
    "bundle": "node esbuild.config.js",
    "dts": "tsup --entry index.ts --entry affine-blog.ts --dts-only --format esm",
    "prepublish": "pnpm build && cp ../../README.md dist/README.md"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.4",
    "common-tags": "^1.8.2",
    "delta-to-md": "workspace:*",
    "esbuild": "^0.23.0",
    "tsup": "^8.2.4",
    "typescript": "5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "yjs": "^13.6.18"
  },
  "type": "module",
  "exports": {
    ".": "./index.ts",
    "./affine-blog": "./affine-blog.ts"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./affine-blog": {
        "import": "./dist/affine-blog.js",
        "types": "./dist/affine-blog.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@blocksuite/blocks": "0.17.0-canary-202408121434-8bc42f0"
  }
}
